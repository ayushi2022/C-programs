#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n,i;
	cout<<"enter num";
	cin>>n;
	int arr[n];
	cout<<"enter array";
	for(i=1;i<=n;i++)
	{
        cin>>arr[i];
	}
	for(i=1;i<=n;i++)
	{
       for(int j=i+1;j<=n;j++)
	   {
		   if(arr[i]==arr[j])
		   {
			   cout<<i;
			   return 0;
		   }
	   }
	}
	return 0;
}
